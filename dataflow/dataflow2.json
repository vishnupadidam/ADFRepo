{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Sourcefiles",
						"type": "DatasetReference"
					},
					"name": "sourcefiles",
					"description": "Sou"
				},
				{
					"dataset": {
						"referenceName": "Previousdeltafile",
						"type": "DatasetReference"
					},
					"name": "PreviousDelta",
					"description": "pre"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText2",
						"type": "DatasetReference"
					},
					"name": "Todaydeltafile"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false,",
				"     enableCdc: true,",
				"     mode: 'read',",
				"     skipInitialLoad: false) ~> sourcefiles",
				"source(output(",
				"          Id as string,",
				"          name as string,",
				"          city as string,",
				"          age as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> PreviousDelta",
				"sourcefiles sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Todaydeltafile"
			]
		}
	}
}