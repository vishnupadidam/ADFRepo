{
	"name": "add_updatelogic",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Sourcefiles",
						"type": "DatasetReference"
					},
					"name": "sourcefiles",
					"description": "Sou"
				},
				{
					"dataset": {
						"referenceName": "Previousdeltafile",
						"type": "DatasetReference"
					},
					"name": "PreviousDelta",
					"description": "pre"
				},
				{
					"name": "source1"
				},
				{
					"name": "source2"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText2",
						"type": "DatasetReference"
					},
					"name": "Todaydeltafile"
				}
			],
			"transformations": [
				{
					"name": "SderivedColumn"
				},
				{
					"name": "DderivedColumn2"
				},
				{
					"name": "exists1"
				},
				{
					"name": "Ststus"
				},
				{
					"name": "union1",
					"description": "Combining rows from transformation 'Ststus' and deleted"
				},
				{
					"name": "exists2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Id as short,",
				"          name as string,",
				"          city as string,",
				"          age as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourcefiles",
				"source(output(",
				"          Id as short,",
				"          name as string,",
				"          city as string,",
				"          age as short",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> PreviousDelta",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> source1",
				"source(allowSchemaDrift: true,",
				"     validateSchema: false) ~> source2",
				"sourcefiles derive(Shashcol = sha2(256,columns())) ~> SderivedColumn",
				"PreviousDelta derive(Dhashcol = sha2(256,columns())) ~> DderivedColumn2",
				"SderivedColumn, DderivedColumn2 exists(Shashcol==Dhashcol,",
				"     negate:true,",
				"     broadcast: 'auto')~> exists1",
				"exists1 derive(Status = 'A') ~> Ststus",
				"Ststus union(byName: true)~> union1",
				"DderivedColumn2, SderivedColumn exists(Dhashcol == Shashcol,",
				"     negate:true,",
				"     broadcast: 'auto')~> exists2",
				"union1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Todaydeltafile"
			]
		}
	}
}