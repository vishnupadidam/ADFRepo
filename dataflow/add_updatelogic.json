{
	"name": "add_updatelogic",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "Sourcefiles",
						"type": "DatasetReference"
					},
					"name": "sourcefiles",
					"description": "Sou"
				},
				{
					"dataset": {
						"referenceName": "Previousdeltafile",
						"type": "DatasetReference"
					},
					"name": "PreviousDelta",
					"description": "pre"
				},
				{
					"dataset": {
						"referenceName": "AzureSqlTable1",
						"type": "DatasetReference"
					},
					"name": "source1"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "DelimitedText2",
						"type": "DatasetReference"
					},
					"name": "Todaydeltafile"
				}
			],
			"transformations": [
				{
					"name": "SderivedColumn"
				},
				{
					"name": "DderivedColumn2"
				},
				{
					"name": "exists1"
				},
				{
					"name": "derivedColumn1"
				},
				{
					"name": "Ststus"
				},
				{
					"name": "union1",
					"description": "Combining rows from transformation 'Ststus' and "
				},
				{
					"name": "exists2"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourcefiles",
				"source(output(",
				"          Id as string,",
				"          name as string,",
				"          city as string,",
				"          age as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> PreviousDelta",
				"source(output(",
				"          empid as integer,",
				"          name as string,",
				"          title as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source1",
				"sourcefiles derive(Shashcol = sha2(256, columns())) ~> SderivedColumn",
				"PreviousDelta derive(Dhashcol = sha2(256,columns())) ~> DderivedColumn2",
				"SderivedColumn, DderivedColumn2 exists(Shashcol==Dhashcol,",
				"     negate:true,",
				"     broadcast: 'auto')~> exists1",
				"source1 derive(SQLhash = toBinary(sha2(256,'empid'))) ~> derivedColumn1",
				"exists1 derive(Status = 'A') ~> Ststus",
				"Ststus union(byName: true)~> union1",
				"DderivedColumn2, SderivedColumn exists(Dhashcol == Shashcol,",
				"     negate:true,",
				"     broadcast: 'auto')~> exists2",
				"union1 sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true) ~> Todaydeltafile"
			]
		}
	}
}